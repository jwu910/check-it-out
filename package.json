{
  "name": "check-it-out",
  "version": "3.1.0",
  "description": "Easy git checkout CLI tool.",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "main": "index.js",
  "scripts": {
    "build": "npm run tsc",
    "cz": "npx git-cz",
    "lint": "prettier -c \"**/*.(js|ts)\"",
    "lint:fix": "prettier --write \"**/*.(js|ts)\"",
    "precise-commits": "precise-commits",
    "prepublishOnly": "npm run build",
    "start": "node index.js",
    "tsc": "tsc",
    "watch": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jwu910/check-it-out.git"
  },
  "keywords": [
    "branch",
    "checkout",
    "cli",
    "easy",
    "git",
    "git-branch",
    "git-checkout",
    "github",
    "tool"
  ],
  "bin": {
    "checkit": "index.js",
    "cio": "index.js"
  },
  "engines": {
    "node": ">=6"
  },
  "author": "Joshua Wu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jwu910/check-it-out/issues"
  },
  "homepage": "https://checkit.club",
  "dependencies": {
    "blessed": "^0.1.81",
    "chalk": "^2.3.1",
    "clipboardy": "^2.1.0",
    "configstore": "^3.1.2",
    "strip-ansi": "^5.2.0",
    "update-notifier": "^2.3.0"
  },
  "devDependencies": {
    "@types/blessed": "^0.1.19",
    "@types/configstore": "^5.0.1",
    "@types/node": "^17.0.33",
    "cz-conventional-changelog": "^2.1.0",
    "danger": "^9.2.1",
    "husky": "^1.1.2",
    "lodash": "^4.17.15",
    "precise-commits": "^1.0.2",
    "prettier": "^2.8.4",
    "typescript": "^4.6.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precise-commits"
    }
  }
}
