{
  "name": "check-it-out",
  "version": "1.0.2",
  "description": "Easy git checkout CLI tool.",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "main": "index.js",
  "scripts": {
    "build": "gulp build",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "format": "prettier --write \"**/*.js\"",
    "lint:fix": "eslint --config .eslintrc.json ./ --fix",
    "lint": "eslint --config .eslintrc.json ./",
    "precise-commits": "precise-commits",
    "precommit": "npm run precise-commits",
    "prepublishOnly": "npm run build",
    "start": "node index.js",
    "test": "npm run lint",
    "watch": "gulp watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jwu910/check-it-out.git"
  },
  "keywords": [
    "branch",
    "checkout",
    "cli",
    "easy",
    "git",
    "git-branch",
    "git-checkout",
    "github",
    "tool"
  ],
  "bin": {
    "checkit": "index.js",
    "cio": "index.js"
  },
  "engines": {
    "node": ">=4"
  },
  "author": "Joshua Wu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jwu910/check-it-out/issues"
  },
  "homepage": "https://checkit.club",
  "dependencies": {
    "blessed": "^0.1.81",
    "chalk": "^2.3.1",
    "configstore": "^3.1.2",
    "update-notifier": "^2.3.0"
  },
  "devDependencies": {
    "all-contributors-cli": "^4.11.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "danger": "^3.7.19",
    "eslint": "^5.7.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "husky": "^1.1.2",
    "lodash": "^4.17.10",
    "precise-commits": "^1.0.2",
    "prettier": "^1.13.7",
    "prettier-eslint": "^8.8.2"
  }
}
